# On notifie si le mot de passe de MySql a été changé
ifeq ($(Mysql_Mdp_Root), mdptropbiensecuretavumagueule)
    $(error Le mot de passe root de MySql doit être changé dans le fichier mdp_root_mysql.conf)
endif

# On notifie si le mot de passe de MySql a été changé
ifeq ($(Mysql_Mdp_Root), )
